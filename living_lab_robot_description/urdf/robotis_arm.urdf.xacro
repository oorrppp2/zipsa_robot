<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <link name="robotis_arm_base">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/robotis_arm_base.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/robotis_arm_base.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="arm_base_joint" type="prismatic">
        <parent link="elv_frame_assy"/>
        <child link="robotis_arm_base"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.05" upper="0.08" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="arm_base_joint" />

