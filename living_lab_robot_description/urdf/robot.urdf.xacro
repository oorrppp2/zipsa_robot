<?xml version="1.0" ?>
<robot name="living_lab_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find living_lab_robot_description)/urdf/gazebo.urdf.xacro"/>
    <xacro:include filename="$(find living_lab_robot_description)/urdf/transmission.urdf.xacro"/>

    <link name="base_footprint">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.001 0.001 0.001" />
            </geometry>
        </visual>
    </link>

    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/omni_base.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/omni_base.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="30.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="base_link_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.095" rpy="0 0 0"/>
    </joint>

    <link name="elv_base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/elv_base.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/elv_base.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="elv_base_rotate_joint" type="revolute">
        <parent link="base_link"/>
        <child link="elv_base_link"/>
        <origin xyz="0 0 0.21" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="elv_base_rotate_joint" />

    <link name="elv_mount_link">
        <visual>
            <origin xyz="0 0 -0.07" rpy="0 ${pi/2} 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/elv_mount.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 ${pi/2} 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/elv_mount.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="elv_base_trans_joint" type="prismatic">
        <parent link="elv_base_link"/>
        <child link="elv_mount_link"/>
        <origin xyz="0.01 0 0.6865" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.5" upper="0.0" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="elv_base_trans_joint" />

    <link name="projector_pan">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/proj_pan.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/proj_pan.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="projector_pan_joint" type="revolute">
        <parent link="elv_base_link"/>
        <child link="projector_pan"/>
        <origin xyz="-0.025 0 0.71" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="projector_pan_joint" />

    <link name="projector_tilt">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/proj_tilt.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/proj_tilt.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="projector_tilt_joint" type="revolute">
        <parent link="projector_pan"/>
        <child link="projector_tilt"/>
        <origin xyz="0 0 0.0785" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="projector_tilt_joint" />

    <link name="arm_link1">
        <visual>
            <origin xyz="0.09 0 0" rpy="0 ${pi/2} 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0.09 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="arm_fixed_base" type="fixed">
        <parent link="elv_mount_link"/>
        <child link="arm_link1"/>
        <origin xyz="0.01 0 -0.03" rpy="0 0 0"/>
    </joint>

    <link name="arm_link2">
        <visual>
            <origin xyz="-0.036 0 0" rpy="${-pi/2} 0 ${-pi/2}"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link2.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link2.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="arm_joint1" type="revolute">
        <parent link="arm_link1"/>
        <child link="arm_link2"/>
        <origin xyz="0.128 0 0" rpy="0 0 0"/>
         <axis xyz="1 0 0"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="arm_joint1" />

    <link name="arm_link3">
        <visual>
            <origin xyz="-0.069 0 0" rpy="${-pi/2} 0 ${-pi/2}"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link3.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link3.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="arm_joint2" type="revolute">
        <parent link="arm_link2"/>
        <child link="arm_link3"/>
        <origin xyz="0.033 0 0" rpy="0 0 0"/>
         <axis xyz="0 1 0"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="arm_joint2" />

    <link name="arm_link4">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link4.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link4.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="arm_joint3" type="revolute">
        <parent link="arm_link3"/>
        <child link="arm_link4"/>
        <origin xyz="0.264 0 0.03" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="arm_joint3" />

    <link name="arm_link5">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link5.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link5.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="arm_joint4" type="revolute">
        <parent link="arm_link4"/>
        <child link="arm_link5"/>
        <origin xyz="0.03 0 0.286" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="arm_joint4" />

    <link name="arm_link6">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link6.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/arm_link6.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="arm_joint5" type="revolute">
        <parent link="arm_link5"/>
        <child link="arm_link6"/>
        <origin xyz="0 0 0.035" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="arm_joint5" />

    <link name="end_effector">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="end_effector_joint" type="revolute">
        <parent link="arm_link6"/>
        <child link="end_effector"/>
        <origin xyz="0 0 0.124" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="${-90/180*pi}" upper="${90/180*pi}" velocity="1.0" effort="1000"/>
    </joint>
    <xacro:insert_position_transmission name="end_effector_joint" />


    <!-- Laser Scanner -->
    <link name="hokuyo_link">
        <visual>
            <origin xyz="-0.04 0.03 -0.05" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/hokuyo.dae" scale="1 1 1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://living_lab_robot_description/meshes/hokuyo.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="hokuyo_mount_joint" type="fixed">
        <parent link="base_link"/>
        <child link="hokuyo_link"/>
        <origin xyz="0.21 0 0.15" rpy="0 0 0"/>
    </joint>




</robot>